import java.util.Scanner;
public class OddEven
{
    public static void main(String[] args)
    {
        int num;
        Scanner reader= new Scanner(System.in);
        System.out.println("Enter Number");
        num=reader.nextInt();
        if(num/2 == 0)
        {
            System.out.println("Even");
        }
        else{
        System.out.println("Odd");
        }
    }
}
