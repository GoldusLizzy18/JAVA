import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        Scanner input=new Scanner(System.in);
        System.out.println("enter the limit:");
        int limit=input.nextInt();
        int fact=1;
        
        for(int i=1;i<=limit;i++){
            fact=fact*i;
        }    
            System.out.println(fact);
    }   
}
